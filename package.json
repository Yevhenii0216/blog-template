    "rehype-code-titles": "^1.0.3",
    "rehype-prism-plus": "1.3.2",
    "rehype-slug": "5.0.1",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "rss": "^1.2.2",
    "sharp": "0.30.3",
