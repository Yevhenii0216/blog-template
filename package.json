{
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "next dev",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "yarn sitemap && yarn rss && yarn og",
    "sitemap": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-sitemap.mjs",
    "rss": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-rss.mjs",
    "og": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-og-images.mjs",
    "new:post": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/compose.mjs",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky install",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "export": "next export",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx}\"",
    "type-check": "tsc",
    "commit": "cz",
    "prisma:studio": "dotenv -e .env.local -- npx prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^3.11.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
