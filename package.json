    "@fontsource/jetbrains-mono": "^4.5.3",
    "@fontsource/noto-sans-tc": "^4.5.4",
    "@prisma/client": "^3.10.0",
    "@react-spring/web": "^9.4.3",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "10.4.2",
    "contentlayer": "0.1.1",
    "esbuild": "^0.13.13",
    "eslint-plugin-cypress": "^2.12.1",
    "framer-motion": "^6.2.8",
    "github-slugger": "^1.3.0",
    "googleapis": "^95.0.0",
    "gray-matter": "^4.0.2",
    "image-size": "1.0.1",
    "lodash": "^4.17.21",
    "mdx-bundler": "8.0.1",
    "next": "12.1.0",
    "next-auth": "^4.2.1",
    "next-contentlayer": "0.1.1",
    "next-pwa": "^5.4.4",
    "next-themes": "^0.1.1",
    "next-translate": "^1.3.4",
    "postcss": "^8.4.5",
    "preact": "^10.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-loading-skeleton": "^3.0.3",
    "react-toastify": "^8.2.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "6.1.1",
    "rehype-citation": "^0.2.0",
    "rehype-katex": "^6.0.2",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.1.3",
    "rehype-slug": "5.0.1",
    "remark": "^14.0.2",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "sharp": "^0.28.3",
    "smoothscroll-polyfill": "^0.4.4",
    "swr": "^1.2.2",
    "tailwindcss": "^3.0.18",
    "unist-util-visit": "^4.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.1.0",
    "@svgr/webpack": "6.2.1",
    "@types/react": "17.0.39",
    "@types/tailwindcss": "2.2.4",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "cross-env": "^7.0.3",
    "cypress": "^9.5.0",
    "dedent": "^0.7.0",
    "eslint": "^7.29.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.3.0",
