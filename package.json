    "image-size": "1.0.1",
    "next": "12.1.5",
    "next-auth": "^4.3.3",
    "next-contentlayer": "0.1.1",
    "next-themes": "^0.1.1",
    "next-translate": "^1.3.5",
    "postcss": "^8.4.12",
    "preact": "10.7.1",
    "prism-react-renderer": "^1.3.1",
    "puppeteer": "13.5.1",
    "qs": "^6.10.3",
    "react": "17.0.2",
